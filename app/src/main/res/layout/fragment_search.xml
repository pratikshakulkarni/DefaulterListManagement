<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="psk.com.recyclerview.Search"
    android:background="#fff">


    <EditText
        android:hint="Enter ROll NO"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/Search_id"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp" />

    <Button
        android:text="Search"
        android:id="@+id/search_bth"
        android:textColor="#fff"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="#dfd70d62"
        android:layout_above="@+id/come_name"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp" />

    <TextView
        android:id="@+id/id1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Search_id"
        android:layout_marginTop="56dp"
        android:gravity="center"
        android:textSize="20dp"
        android:text="Name"/>

    <TextView
        android:layout_toRightOf="@+id/id1"
        android:id="@+id/come_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Search_id"
        android:layout_marginTop="56dp"
        android:gravity="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/id2"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sub1"
        android:layout_alignBottom="@+id/sub1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="DBMS"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/sub1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/come_name"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/id3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sub2"
        android:layout_alignBottom="@+id/sub2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="SEPM"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/sub2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sub1"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/id4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sub3"
        android:layout_alignBottom="@+id/sub3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="ISEE"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/sub3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sub2"
        android:layout_marginTop="22dp"
        android:gravity="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/id5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sub4"
        android:layout_alignBottom="@+id/sub4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="CN"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/sub4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sub3"
        android:layout_marginTop="22dp"
        android:gravity="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/id6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sub5"
        android:layout_alignBottom="@+id/sub5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="TOC"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/sub5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sub4"
        android:layout_marginTop="24dp"
        android:gravity="center"
        android:textSize="20dp" />

</RelativeLayout>
